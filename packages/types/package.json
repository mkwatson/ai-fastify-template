{
  "name": "@ai-fastify-template/types",
  "version": "0.1.0",
  "description": "Branded types for compile-time ID safety",
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./property-testing-simple": {
      "import": "./dist/src/property-testing-simple.js",
      "types": "./dist/src/property-testing-simple.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "test": "NODE_OPTIONS='--import tsx/esm' vitest run",
    "test:watch": "NODE_OPTIONS='--import tsx/esm' vitest",
    "test:coverage": "NODE_OPTIONS='--import tsx/esm' vitest run --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ai-fastify-template/config": "workspace:*",
    "expect-type": "^0.17.3"
  },
  "peerDependencies": {
    "zod": "^3.22.4"
  }
}
