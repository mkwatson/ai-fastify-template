#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mark/Projects/ai-fastify-template/node_modules/.pnpm/turbo@2.5.4/node_modules/turbo/bin/node_modules:/Users/mark/Projects/ai-fastify-template/node_modules/.pnpm/turbo@2.5.4/node_modules/turbo/node_modules:/Users/mark/Projects/ai-fastify-template/node_modules/.pnpm/turbo@2.5.4/node_modules:/Users/mark/Projects/ai-fastify-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mark/Projects/ai-fastify-template/node_modules/.pnpm/turbo@2.5.4/node_modules/turbo/bin/node_modules:/Users/mark/Projects/ai-fastify-template/node_modules/.pnpm/turbo@2.5.4/node_modules/turbo/node_modules:/Users/mark/Projects/ai-fastify-template/node_modules/.pnpm/turbo@2.5.4/node_modules:/Users/mark/Projects/ai-fastify-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/turbo@2.5.4/node_modules/turbo/bin/turbo" "$@"
else
  exec node  "$basedir/../.pnpm/turbo@2.5.4/node_modules/turbo/bin/turbo" "$@"
fi
